{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./lib/.tsbuildinfo",
    "types": ["node"],
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@types/",
      "types",
      "documentation/node_modules/@types",
      "documentation/node_modules/@types/",
      "documentation/node_modules/@docusaurus",
      "documentation/node_modules/@docusaurus/",
      "documentation/node_modules/@sentry",
      "documentation/node_modules/@sentry/"
    ],
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "lib"
  },
  "include": [
    "src",
    "sidebars.js",
    "babel.config.js",
    "docusaurus.core.config.js",
    "docusaurus.github.config.js",
    "docusaurus.vercel.config.js",
    "scripts"
  ],
  "exclude": ["**/__tests__/**/*", "src/client"]
}
